{
	"extends": "airbnb",
	
	/*"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"plugins": [
		"react"
	],*/
	"globals": {
		"document": true,
		"localStorage": true
	},
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": 0,
		"react/forbid-prop-types": 0,
		"react/jsx-indent-props": [2, "tab"],
		"react/jsx-indent": [2, "tab"],
		"react/jsx-filename-extension": [1, {
			"extensions": [".js", ".jsx"]
		}]
	}
}